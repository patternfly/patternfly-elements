---
pagination:
    data: elements
    size: 1
    alias: element
permalink: "demo/{{ element }}/index.html"
---
{% set demoConfig = elements[element] %}

<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="PatternFly Elements: A set of community-created web components based on PatternFly design.">
    <link href="/brand/logo/svg/pfe-icon-blue.svg" rel="shortcut icon">
    <title>{{ element }} | PatternFly Elements</title>
    <link rel="preconnect" href="https://unpkg.com">
    <link rel="stylesheet" href="/main.css"/>
    <link rel="stylesheet" href="/core/pfe-core/pfe.min.css">
    <link rel="stylesheet" href="/core/pfe-styles/pfe-layouts.min.css">
    <link rel="stylesheet" href="/core/pfe-styles/red-hat-font.min.css">
    <script type="importmap">
    {
      "imports": {
        "lit": "https://unpkg.com/lit?module",
        "lit/decorators.js": "https://unpkg.com/lit/decorators.js?module",
        "lit/directive.js": "https://unpkg.com/lit/directive.js?module",
        "lit/static-html.js": "https://unpkg.com/lit/static-html.js?module",
        "lit/directives/repeat.js": "https://unpkg.com/lit/directives/repeat.js?module",
        "lit/directives/style-map.js": "https://unpkg.com/lit/directives/style-map.js?module",
        "lit/directives/class-map.js": "https://unpkg.com/lit/directives/class-map.js?module",
        "lit/directives/if-defined.js": "https://unpkg.com/lit/directives/if-defined.js?module",
        "lit/directives/unsafe-html.js": "https://unpkg.com/lit/directives/unsafe-html.js?module",

        "prismjs": "https://unpkg.com/prismjs",
        "marked": "https://unpkg.com/marked?module",

        "@patternfly/pfe-core": "/core/pfe-core/core.js",
        "@patternfly/pfe-core/decorators.js": "/core/pfe-core/decorators.js",
        "@patternfly/pfe-core/decorators/": "/core/pfe-core/decorators/",
        "@patternfly/pfe-core/": "/core/pfe-core/",

        "@patternfly/pfe-autocomplete": "/elements/pfe-autocomplete/pfe-autocomplete.js",
        "@patternfly/pfe-accordion": "/elements/pfe-accordion/pfe-accordion.js",
        "@patternfly/pfe-avatar": "/elements/pfe-avatar/pfe-avatar.js",
        "@patternfly/pfe-badge": "/elements/pfe-badge/pfe-badge.js",
        "@patternfly/pfe-band": "/elements/pfe-band/pfe-band.js",
        "@patternfly/pfe-button": "/elements/pfe-button/pfe-button.js",
        "@patternfly/pfe-card": "/elements/pfe-card/pfe-card.js",
        "@patternfly/pfe-clipboard": "/elements/pfe-clipboard/pfe-clipboard.js",
        "@patternfly/pfe-codeblock": "/elements/pfe-codeblock/pfe-codeblock.js",
        "@patternfly/pfe-collapse": "/elements/pfe-collapse/pfe-collapse.js",
        "@patternfly/pfe-collapse/": "/elements/pfe-collapse/",
        "@patternfly/pfe-content-set": "/elements/pfe-content-set/pfe-content-set.js",
        "@patternfly/pfe-cta": "/elements/pfe-cta/pfe-cta.js",
        "@patternfly/pfe-datetime": "/elements/pfe-datetime/pfe-datetime.js",
        "@patternfly/pfe-dropdown": "/elements/pfe-dropdown/pfe-dropdown.js",
        "@patternfly/pfe-health-index": "/elements/pfe-health-index/pfe-health-index.js",
        "@patternfly/pfe-icon": "/elements/pfe-icon/pfe-icon.js",
        "@patternfly/pfe-icon-panel": "/elements/pfe-icon-panel/pfe-icon-panel.js",
        "@patternfly/pfe-jump-links": "/elements/pfe-jump-links/pfe-jump-links.js",
        "@patternfly/pfe-markdown": "/elements/pfe-markdown/pfe-markdown.js",
        "@patternfly/pfe-modal": "/elements/pfe-modal/pfe-modal.js",
        "@patternfly/pfe-number": "/elements/pfe-number/pfe-number.js",
        "@patternfly/pfe-page-status": "/elements/pfe-page-status/pfe-page-status.js",
        "@patternfly/pfe-primary-detail": "/elements/pfe-primary-detail/pfe-primary-detail.js",
        "@patternfly/pfe-progress-indicator": "/elements/pfe-progress-indicator/pfe-progress-indicator.js",
        "@patternfly/pfe-progress-steps": "/elements/pfe-progress-steps/pfe-progress-steps.js",
        "@patternfly/pfe-readtime": "/elements/pfe-readtime/pfe-readtime.js",
        "@patternfly/pfe-select": "/elements/pfe-select/pfe-select.js",
        "@patternfly/pfe-tabs": "/elements/pfe-tabs/pfe-tabs.js",
        "@patternfly/pfe-toast": "/elements/pfe-toast/pfe-toast.js"

      }
    }
    </script>
    <script async src="https://unpkg.com/es-module-shims@1.3.0/dist/es-module-shims.js"></script>
    <script type="module" src="/elements/pfe-band/pfe-band.js"></script>
    <script type="module" src="{{ demoConfig.module }}"></script>
    <script type="module">
      const container = document.getElementById("shadow-root");
      const shadowRoot = container.attachShadow({ mode: 'open', delegatesFocus: true });
      const template = document.getElementById('template');
      shadowRoot.appendChild(template.content.cloneNode(true));
    </script>
    {%- if demoConfig.script -%}
    <!-- Load the demo's JavaScript -->
    <script type="module">
      {%- include demoConfig.script -%}
    </script>
    {%- endif -%}
  </head>
  <body>
    <main id="shadow-root" data-demo="{{ element }}"></main>
    <template id="template">
      {%- if demoConfig.demo -%}
        {%- include demoConfig.demo -%}
      {%- endif -%}
    </template>
  </body>
</html>
