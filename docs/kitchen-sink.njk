    <style>
      /* Replicate styles from /sites/ and static.redhat.com */
      :root :is(h1, h2, h3) {
        line-height: 1.1;
        margin-top: 22px;
        margin-bottom: 11px;
      }

      p {
        margin:0 0 11px;
        margin-bottom:1.8rem
      }

      main {
        display: grid;
        grid-template-columns: repeat(6, 1fr);
      }

      main > pfe-band {
        grid-column: span 6;
      }

      .one-half {
        grid-column: span 3;
      }

      .one-third {
        grid-column: span 2;
      }

      .flex-footer::part(footer) {
        display: flex;
        flex-flow: row wrap;
        align-items: center;
        gap: 10px;
      }

      .grid-body::part(body) {
        display: grid;
        grid-template-columns: repeat(var(--rep, auto-fit), minmax(200px, 1fr));
        gap: 15px;
      }

      [data-element="button"] {
        --rep: auto-fill;
      }

      .border-card {
        --pfe-card--Border: 1px solid var(--pfe-theme--surface--border, #d2d2d2);
      }

      .border-card h3 {
        margin-top: 0;
      }

      .resource-card [slot="header"] {
        text-transform: uppercase;
      }

      .custom-background {
        background-color: #123D99;
        --theme: saturated;
        --context: saturated;
      }

      .custom-styles {
        --pfe-card--BackgroundPosition: bottom right;
        --pfe-card--BackgroundColor: #e00;
        --theme: saturated;
      }
    </style>
