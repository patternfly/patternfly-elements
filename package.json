{
  "name": "@patternfly/patternfly-elements",
  "description": "Monorepo for the PatternFly Elements project.",
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements"
  },
  "engines": {
    "node": ">=10 <13"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox >= 78",
    "iOS >= 8",
    "ie 11"
  ],
  "scripts": {
    "start": "npm config set quiet=true; eleventy --serve",
    "lerna": "lerna",
    "test": "node scripts/test.js",
    "test:watch": "npm config set group=default && npm config set element=\"*\" && web-test-runner \"elements/*/test/$npm_config_element.spec.js\" --node-resolve --playwright --watch --group=$npm_config_group",
    "test:build:watch": "npm config set element=\"*\" && npm run build \"$npm_config_element\" && npm run test:watch",
    "test:ci": "web-test-runner --node-resolve --playwright",
    "dev": "npm run watch -- --build",
    "watch": "node scripts/watch.js",
    "build": "node scripts/build.js",
    "clean": "rm -rf node_modules elements/*/{node_modules,package-lock.json}",
    "clean:components": "rm -rf elements/*/{dist,_temp}",
    "clean:e2e": "rm -f test/vrt-snapshots/{actual,diff}/desktop_{chrome,ie}/*",
    "clean:docs": "rm -rf _site/* && rm -fdr docs/components/*/",
    "clean:all": "npm-run-all --parallel clean:components clean:e2e",
    "bootstrap": "lerna bootstrap --hoist",
    "start:docs": "eleventy --serve",
    "prebuild:docs": "npm-run-all --serial clean:docs --parallel todos polyfills",
    "build:docs": "eleventy",
    "release": "bash scripts/release.sh",
    "postinstall": "export FORCE_COLOR=3; npm run bootstrap",
    "license-update": "node scripts/license-update.js",
    "storybook": "start-storybook -c .storybook -p 9001 -s ./elements --quiet",
    "build-storybook": "build-storybook -c .storybook -o ./storybook --quiet --loglevel error",
    "new": "cd elements && yo --no-insight --no-update-notifier ../generators/element --type=pfelement && npm run bootstrap",
    "new:test": "yo ./generators/test",
    "leftover-check": "bash scripts/leftover-check.sh",
    "demo": "npm run build -- --preview",
    "live-demo": "npm run dev",
    "pr": "node scripts/open-pr.js",
    "pree2e": "npm run clean:e2e",
    "e2e": "wdio wdio.conf.js",
    "todos": "leasot -x --reporter json 'elements/*/src/*.js' > docs/_data/todos.json",
    "polyfills": "leasot -x --reporter json --tags 'POLYFILL' 'elements/*/src/*.js' | ./node_modules/node-jq/bin/jq 'map(select(.tag == \"POLYFILL\"))' > docs/_data/polyfills.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-checkout": "npm run clean:components && npm run leftover-check"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.8.3",
    "@esm-bundle/chai": "^4.3.4",
    "@open-wc/testing": "^2.5.32",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-backgrounds": "^5.3.8",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-notes": "^5.3.8",
    "@storybook/addon-viewport": "^6.0.26",
    "@storybook/cli": "^5.3.8",
    "@storybook/polymer": "^5.3.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@wdio/browserstack-service": "^6.1.10",
    "@wdio/cli": "^6.1.12",
    "@wdio/local-runner": "^7.7.2",
    "@wdio/mocha-framework": "^7.0.7",
    "@wdio/spec-reporter": "^7.7.0",
    "@wdio/sync": "^7.0.3",
    "@web/test-runner": "^0.13.5",
    "@web/test-runner-playwright": "^0.8.4",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "autoprefixer": "^10.2.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-standalone": "^6.26.0",
    "browser-sync": "^2.24.6",
    "caniuse-db": "^1.0.30001183",
    "chalk": "^4.1.0",
    "chromedriver": "^90.0.1",
    "clean-html": "^1.5.0",
    "compression": "^1.7.4",
    "css-scss": "^0.1.1",
    "decomment": "^0.9.1",
    "del": "^6.0.0",
    "dialog-polyfill": "^0.5.6",
    "dotenv": "^10.0.0",
    "foodoc": "0.0.9",
    "ghp": "^1.3.1",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-banner": "^0.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-if": "^3.0.0",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-match": "^1.1.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "inquirer": "^8.0.0",
    "leasot": "^12.0.0",
    "lerna": "^4.0.0",
    "lit-html": "^1.1.2",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lodash.template": "^4.5.0",
    "lorem-ipsum": "^2.0.3",
    "markdown-it": "^12.0.5",
    "markdown-it-anchor": "^8.1.2",
    "markdown-it-container": "^3.0.0",
    "node-jq": "^1.12.0",
    "node-sass": "^6.0.0",
    "npm-merge-driver": "^2.3.6",
    "npm-run-all": "^4.1.5",
    "open": "^8.0.4",
    "playwright": "^1.9.2",
    "polymer-webpack-loader": "^2.0.3",
    "postcss": "^8.2.4",
    "postcss-custom-properties": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-uglify": "^6.0.4",
    "sassdoc": "^2.7.3",
    "shelljs": "^0.8.4",
    "sinon": "^11.1.1",
    "util": "^0.12.1",
    "wct-browser-legacy": "^1.0.2",
    "wdio-image-comparison-service": "^2.0.0",
    "web-component-tester": "^6.9.2",
    "whatwg-fetch": "^3.0.0",
    "yargs": "^16.0.3",
    "yeoman-generator": "^4.12.0",
    "yo": "^4.1.0",
    "yosay": "^2.0.2"
  },
  "private": true
}
