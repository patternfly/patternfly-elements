{
  "name": "@patternfly/patternfly-elements",
  "description": "Monorepo for the PatternFly Elements project.",
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements"
  },
  "engines": {
    "node": ">=10 <13"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox >= 78",
    "iOS >= 8",
    "ie 11"
  ],
  "scripts": {
    "start": "spandx",
    "lerna": "lerna",
    "test": "node scripts/test.js",
    "dev": "npm run watch -- --build",
    "watch": "node scripts/watch.js",
    "prebuild": "npm run doc-listing-inject",
    "build": "node scripts/build.js",
    "postbuild": "rm -rf examples/node_modules && mkdir examples/node_modules && cp -r node_modules/@webcomponents node_modules/requirejs examples/node_modules/",
    "clean": "rm -rf node_modules package-lock.json elements/*/{node_modules,package-lock.json,_temp} test/vrt-snapshots",
    "bootstrap": "lerna bootstrap --hoist",
    "deploy-docs": "bash scripts/deploy-docs.sh",
    "release": "bash scripts/release.sh",
    "postinstall": "export FORCE_COLOR=3; npm run bootstrap",
    "test-suite-inject": "node scripts/test-suite-inject.js",
    "doc-listing-inject": "npm run leftover-check && node scripts/doc-listing-inject.js",
    "license-update": "node scripts/license-update.js",
    "storybook": "start-storybook -c .storybook -p 9001 -s ./elements --quiet",
    "build-storybook": "build-storybook -c .storybook -o storybook --quiet --loglevel error",
    "gh-pages-storybook": "storybook-to-ghpages",
    "deploy-storybook": "npm run build-storybook && npm run gh-pages-storybook",
    "new": "cd elements && yo --no-insight --no-update-notifier ../generators/element --type=pfelement && npm run post-new",
    "post-new": "npm run test-suite-inject && npm run doc-listing-inject && npm run bootstrap",
    "leftover-check": "bash scripts/leftover-check.sh",
    "demo": "npm run build -- --preview",
    "live-demo": "npm run dev",
    "pr": "node scripts/open-pr.js",
    "e2e": "wdio wdio.conf.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-suite-inject > /dev/null && pretty-quick --staged",
      "post-checkout": "npm run leftover-check"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-backgrounds": "^5.3.8",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-notes": "^5.3.8",
    "@storybook/addon-viewport": "^6.0.26",
    "@storybook/cli": "^5.3.8",
    "@storybook/polymer": "^5.3.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@wdio/browserstack-service": "^6.1.10",
    "@wdio/cli": "^6.1.12",
    "@wdio/local-runner": "^6.1.12",
    "@wdio/mocha-framework": "^7.0.7",
    "@wdio/spec-reporter": "^7.0.0",
    "@wdio/sync": "^7.0.3",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "autoprefixer": "^10.2.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-standalone": "^6.26.0",
    "browser-sync": "^2.24.6",
    "caniuse-db": "^1.0.30001183",
    "chalk": "^4.1.0",
    "chromedriver": "^88.0.0",
    "clean-html": "^1.5.0",
    "compression": "^1.7.4",
    "css-scss": "^0.1.1",
    "decomment": "^0.9.1",
    "dialog-polyfill": "^0.5.6",
    "dotenv": "^8.2.0",
    "ghp": "^1.3.1",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-banner": "^0.1.3",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-if": "^3.0.0",
    "gulp-match": "^1.1.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-globbing": "^0.0.2",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "http-server": "^0.12.3",
    "husky": "^5.0.9",
    "inquirer": "^8.0.0",
    "leasot": "^11.5.0",
    "lerna": "^4.0.0",
    "lit-html": "^1.1.2",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lodash.template": "^4.5.0",
    "lorem-ipsum": "^2.0.3",
    "merge-stream": "^2.0.0",
    "node-sass": "^5.0.0",
    "npm-merge-driver": "^2.3.6",
    "npm-run-all": "^4.1.5",
    "open": "^7.4.0",
    "polymer-webpack-loader": "^2.0.3",
    "postcss": "^8.2.4",
    "postcss-custom-properties": "^11.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "requirejs": "^2.3.5",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "sassdoc": "^2.7.3",
    "shelljs": "^0.8.4",
    "spandx": "^2.0.1",
    "util": "^0.12.1",
    "vue": "^2.6.11",
    "wct-browser-legacy": "^1.0.2",
    "wdio-image-comparison-service": "^2.0.0",
    "web-component-tester": "^6.9.2",
    "whatwg-fetch": "^3.0.0",
    "yargs": "^16.0.3",
    "yeoman-generator": "^4.4.0",
    "yo": "^1.8.5",
    "yosay": "^2.0.2"
  },
  "dependencies": {
    "gh-pages": "^2.2.0"
  },
  "private": true
}
