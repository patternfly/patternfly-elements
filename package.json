{
  "name": "@patternfly/patternfly-elements",
  "description": "Monorepo for the PatternFly Elements project.",
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements"
  },
  "scripts": {
    "start": "npm run doc-listing-inject && spandx",
    "lerna": "lerna",
    "test": "bash scripts/test.sh",
    "dev": "bash scripts/dev.sh",
    "build": "bash scripts/build.sh",
    "clean": "rm -rf node_modules package-lock.json {elements,themes}/*/{node_modules,package-lock.json}",
    "bootstrap": "lerna bootstrap --hoist",
    "deploy-docs": "bash scripts/deploy-docs.sh",
    "release": "bash scripts/release.sh",
    "postinstall": "npm run bootstrap && npm run build",
    "test-suite-inject": "node scripts/test-suite-inject.js",
    "doc-listing-inject": "node scripts/doc-listing-inject.js",
    "license-update": "node scripts/license-update.js",
    "storybook": "start-storybook -c .storybook -p 9001 -s ./elements",
    "build-storybook": "build-storybook -c .storybook -o .storybook_out",
    "gh-pages-storybook": "storybook-to-ghpages",
    "deploy-storybook": "npm run build-storybook && npm run gh-pages-storybook",
    "new": "cd elements && yo --no-insight --no-update-notifier pfelement --type=pfelement && npm run post-new",
    "post-new": "npm run test-suite-inject && npm run doc-listing-inject && npm run bootstrap",
    "leftover-check": "bash scripts/leftover-check.sh",
    "demo": "npm run build && npm run start",
    "live-demo": "npm-run-all --parallel dev start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-suite-inject > /dev/null && pretty-quick --staged",
      "post-checkout": "npm run leftover-check"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.6.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@patternfly/patternfly": "^2.33.0",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-backgrounds": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/cli": "^5.2.1",
    "@storybook/polymer": "^5.2.1",
    "@storybook/storybook-deployer": "^2.8.1",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.24.6",
    "chalk": "^2.4.1",
    "clean-html": "^1.5.0",
    "compression": "^1.7.4",
    "css-scss": "^0.1.1",
    "decomment": "^0.9.2",
    "del": "^3.0.0",
    "dialog-polyfill": "^0.4.10",
    "generator-pfelement": "^1.3.0",
    "ghp": "^1.3.1",
    "global": "^4.4.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-banner": "^0.1.3",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-if": "^3.0.0",
    "gulp-match": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-globbing": "^0.0.2",
    "gulp-sequence": "^1.0.0",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-strip-css-comments": "^2.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-trim": "^1.0.2",
    "gulp-uglify": "^3.0.1",
    "husky": "^1.3.1",
    "lerna": "^3.16.4",
    "lit-html": "^1.1.2",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lorem-ipsum": "^1.0.6",
    "merge-stream": "^2.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "polymer-webpack-loader": "^2.0.3",
    "postcss-custom-properties": "^8.0.11",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "requirejs": "^2.3.6",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-uglify": "^4.0.0",
    "spandx": "^2.0.1",
    "strip-css-comments": "^3.0.0",
    "trim": "^0.0.1",
    "wct-browser-legacy": "^1.0.2",
    "web-component-tester": "^6.9.2",
    "whatwg-fetch": "^3.0.0",
    "yo": "^1.8.5"
  },
  "dependencies": {
    "gh-pages": "^2.1.1"
  },
  "private": true
}
