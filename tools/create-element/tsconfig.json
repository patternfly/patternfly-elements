{
  "extends": "../../tsconfig.settings.json",
  "exclude": [
    "bin",
    "node_modules",
    "templates",
    "templates/**/*",
    "**/node_modules/**",
    "*.tsbuildinfo"
  ],
  "include": [
    "."
  ],
  "compilerOptions": {
    "module": "es2020",
    "target": "es2020",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "emitDeclarationOnly": false
  }
}
