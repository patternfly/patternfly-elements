{
  "name": "@patternfly/pfe-cli",
  "version": "0.0.0",
  "type": "module",
  "description": "Command line utilities for development with PatternFly Elements core and tools",
  "bin": {
    "pfe": "./cli.js"
  },
  "main": "cli.js",
  "module": "cli.js",
  "exports": {
    ".": {
      "default": "./cli.js",
      "typescript": "./cli.ts"
    }
  },
  "imports": {
    "#lib/*": "./lib/*"
  },
  "keywords": [
    "patternfly",
    "web-components",
    "cli",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements",
    "directory": "tools/pfe-cli"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18"
  },
  "contributors": [
    "Benny Powers <bennyp@redhat.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patternfly/patternfly-elements/issues"
  },
  "files": [
    "!*.ts",
    "commands/generate/templates/**/*",
    "**/*.LEGAL.txt",
    "**/*.{njk,html,md,css,woff,woff2}",
    "**/*.{js,cjs,d.ts,map}"
  ],
  "dependencies": {
    "@lit-labs/analyzer": "^0.9.2",
    "@lit-labs/gen-manifest": "^0.2.5",
    "@types/yargs": "^17.0.24",
    "case": "1.6.3",
    "chalk": "^5.3.0",
    "custom-elements-manifest": "^2.0.0",
    "dotenv": "^16.3.1",
    "execa": "^8.0.1",
    "glob": "^10.3.3",
    "prompts": "2.4.2",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.1",
    "ts-patch": "3.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "yargs": "^17.7.2"
  }
}
