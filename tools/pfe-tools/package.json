{
  "name": "@patternfly/pfe-tools",
  "version": "1.0.0-next.11",
  "type": "module",
  "description": "Development and build tools for PatternFly Elements and related projects",
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements",
    "directory": "tools/pfe-tools"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14"
  },
  "exports": {
    "./11ty": "./11ty/index.js",
    "./*.js": "./*.js",
    "./*": "./*.js"
  },
  "contributors": [
    "Kyle Buchanan <kylebuch8@gmail.com>, (https://github.com/kylebuch8)",
    "Benny Powers <bennyp@redhat.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patternfly/patternfly-elements/issues"
  },
  "files": [
    "!*.ts",
    "custom-elements.json",
    "**/*.LEGAL.txt",
    "**/*.css",
    "**/*.md",
    "**/*.d.ts",
    "**/*.js",
    "**/*.js.map"
  ],
  "scripts": {
    "build": "tsc -b .",
    "rebuild": "rm tsconfig.tsbuildinfo && tsc -b .",
    "clean": "git clean -fX -e node_modules -e tsconfig.tsbuildinfo",
    "dev": "node _scripts/dev.js",
    "prepublishOnly": "npm run rebuild"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@custom-elements-manifest/analyzer": "^0.5.6",
    "@open-wc/testing": "^3.1.2",
    "@orchidjs/eleventy-plugin-ids": "^0.1.1",
    "@percy/cli": "^1.0.0-beta.71",
    "@percy/playwright": "^1.0.1",
    "@playwright/test": "^1.20.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/estree": "^0.0.51",
    "@types/glob": "7.2.0",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.0",
    "@types/nunjucks": "^3.2.1",
    "@types/sass": "^1.43.1",
    "@types/sinon": "^10.0.9",
    "@types/uglifycss": "^0.0.7",
    "@types/yargs": "^17.0.9",
    "@vaadin/split-layout": "^23.0.0",
    "@web/dev-server": "^0.1.29",
    "@web/dev-server-esbuild": "^0.2.14",
    "@web/dev-server-import-maps": "^0.0.6",
    "@web/dev-server-rollup": "^0.3.15",
    "@web/test-runner": "^0.13.26",
    "@web/test-runner-junit-reporter": "^0.4.11",
    "@web/test-runner-playwright": "^0.8.8",
    "api-viewer-element": "^1.0.0-pre.5 || ^1.0.0",
    "cem-plugin-module-file-extensions": "^0.0.5",
    "cem-plugin-readonly": "^0.0.5",
    "clean-css": "^5.2.4",
    "compression": "^1.7.4",
    "custom-elements-manifest": "^1.0.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.0",
    "eleventy-plugin-toc": "^1.1.5",
    "esbuild": "^0.14.27",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-plugin-lit-css": "^1.2.3",
    "esbuild-plugin-minify-html-literals": "^1.0.1",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "html-include-element": "^0.3.0",
    "lit": "^2.1.2",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "playwright": "^1.20.0",
    "playwright-lighthouse": "^2.0.4",
    "pwa-helpers": "^0.9.1",
    "rollup-plugin-lit-css": "^3.2.1",
    "sass": "^1.49.9",
    "sinon": "^13.0.0",
    "ts-lit-plugin": "^1.2.1",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
    "urlpattern-polyfill": "^1.0.0-rc1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.20.0",
    "@types/dedent": "^0.7.0",
    "patch-package": "^6.4.7"
  }
}
