{
  "files": ["./declaration.d.ts"],
  "exclude": [
    "node_modules",
    "**/node_modules/**",
    "**/demo/*.js",
    "**/demo/*.d.ts",
    "**/_temp/**/*",
    "**/custom-elements-manifest.config.js",
    "**/*.story.*",
    "**/test/*_e2e*"
  ],
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "composite": true,
    "declaration": true,
    "downlevelIteration": true,
    "emitDeclarationOnly": true,
    "experimentalDecorators": true,
    "importHelpers": false,
    "incremental": true,
    "inlineSources": true,
    "module": "es2020",
    "moduleResolution": "Node",
    "noEmitOnError": false,
    "preserveValueImports": true,
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
    "useDefineForClassFields": false,
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ],
    "plugins": [
      { "name": "typescript-lit-html-plugin" },
      { "name": "ts-lit-plugin" }
    ],
    "lib": [
      "DOM.iterable",
      "DOM",
      "es2015.iterable",
      "es2020",
      "es5",
      "es6",
      "ScriptHost"
    ],
    "paths": {
      "@custom-elements-manifest/analyzer": ["@types/custom-elements-manifest__analyzer"],
      "@custom-elements-manifest/analyzer/*": ["@types/custom-elements-manifest__analyzer/*"],
      "@patternfly/*": [
        "./core/*",
        "./elements/*"
      ],
      "@patternfly/patternfly/*": ["@patternfly/patternfly/*"],
      "@patternfly/pfe-accordion": ["./elements/pfe-accordion/pfe-accordion.ts"],
      "@patternfly/pfe-autocomplete": ["./elements/pfe-autocomplete/pfe-autocomplete.ts"],
      "@patternfly/pfe-avatar": ["./elements/pfe-avatar/pfe-avatar.ts"],
      "@patternfly/pfe-badge": ["./elements/pfe-badge/pfe-badge.ts"],
      "@patternfly/pfe-band": ["./elements/pfe-band/pfe-band.ts"],
      "@patternfly/pfe-button": ["./elements/pfe-button/pfe-button.ts"],
      "@patternfly/pfe-card": ["./elements/pfe-card/pfe-card.ts"],
      "@patternfly/pfe-clipboard": ["./elements/pfe-clipboard/pfe-clipboard.ts"],
      "@patternfly/pfe-codeblock": ["./elements/pfe-codeblock/pfe-codeblock.ts"],
      "@patternfly/pfe-collapse": ["./elements/pfe-collapse/pfe-collapse.ts"],
      "@patternfly/pfe-cta": ["./elements/pfe-cta/pfe-cta.ts"],
      "@patternfly/pfe-datetime": ["./elements/pfe-datetime/pfe-datetime.ts"],
      "@patternfly/pfe-dropdown": ["./elements/pfe-dropdown/pfe-dropdown.ts"],
      "@patternfly/pfe-health-index": ["./elements/pfe-health-index/pfe-health-index.ts"],
      "@patternfly/pfe-icon": ["./elements/pfe-icon/pfe-icon.ts"],
      "@patternfly/pfe-icon-panel": ["./elements/pfe-icon-panel/pfe-icon-panel.ts"],
      "@patternfly/pfe-jump-links": ["./elements/pfe-jump-links/pfe-jump-links.ts"],
      "@patternfly/pfe-markdown": ["./elements/pfe-markdown/pfe-markdown.ts"],
      "@patternfly/pfe-modal": ["./elements/pfe-modal/pfe-modal.ts"],
      "@patternfly/pfe-number": ["./elements/pfe-number/pfe-number.ts"],
      "@patternfly/pfe-page-status": ["./elements/pfe-page-status/pfe-page-status.ts"],
      "@patternfly/pfe-primary-detail": ["./elements/pfe-primary-detail/pfe-primary-detail.ts"],
      "@patternfly/pfe-progress-indicator": ["./elements/pfe-progress-indicator/pfe-progress-indicator.ts"],
      "@patternfly/pfe-progress-steps": ["./elements/pfe-progress-steps/pfe-progress-steps.ts"],
      "@patternfly/pfe-readtime": ["./elements/pfe-readtime/pfe-readtime.ts"],
      "@patternfly/pfe-select": ["./elements/pfe-select/pfe-select.ts"],
      "@patternfly/pfe-tabs": ["./elements/pfe-tabs/pfe-tabs.ts"],
      "@patternfly/pfe-toast": ["./elements/pfe-toast/pfe-toast.ts"],
      "@patternfly/pfe-tools/*": ["./tools/pfe-tools/*"],
      "marked": ["@types/marked"]
    }
  },
  "references": [
    { "path": "./core/pfe-core" },
    { "path": "./elements/pfe-accordion" },
    { "path": "./elements/pfe-autocomplete" },
    { "path": "./elements/pfe-badge" },
    { "path": "./elements/pfe-avatar" },
    { "path": "./elements/pfe-band" },
    { "path": "./elements/pfe-button" },
    { "path": "./elements/pfe-card" },
    { "path": "./elements/pfe-clipboard" },
    { "path": "./elements/pfe-codeblock" },
    { "path": "./elements/pfe-collapse" },
    { "path": "./elements/pfe-cta" },
    { "path": "./elements/pfe-datetime" },
    { "path": "./elements/pfe-dropdown" },
    { "path": "./elements/pfe-health-index" },
    { "path": "./elements/pfe-icon" },
    { "path": "./elements/pfe-icon-panel" },
    { "path": "./elements/pfe-jump-links" },
    { "path": "./elements/pfe-markdown" },
    { "path": "./elements/pfe-modal" },
    { "path": "./elements/pfe-number" },
    { "path": "./elements/pfe-page-status" },
    { "path": "./elements/pfe-primary-detail" },
    { "path": "./elements/pfe-progress-indicator" },
    { "path": "./elements/pfe-progress-steps" },
    { "path": "./elements/pfe-readtime" },
    { "path": "./elements/pfe-select" },
    { "path": "./elements/pfe-tabs" },
    { "path": "./elements/pfe-toast" }
  ]
}
